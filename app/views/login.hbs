<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<link rel="stylesheet" href="/css/style.css" />
<script type="text/javascript" src="/js/libs.js"></script>
</head>
<body>

    <header> 
        <div class="container">
            <div class="row">
                <div class="col-xs-12 col-md-8">
                    <div id="logo">
                        <a href="#">
                            <img src="assets/img/logo.png" alt="Test Logo">
                        </a>
                    </div>
                </div>
                <div class="col-xs-12 col-md-4">
                    <div id="header-buttons">
                        <div id="js-auth-dialog"></div>
                    </div>
                </div>
            </div>
        </div>
    </header>

    <main class="main">
        <div class="login-form-wrapper">
            {{#if message}}
                <div class="error-messages">{{ message }}</div>
            {{/if}}
            <div id="js-login-form"></div>
        </div>
    </main>

    <footer>

    </footer>

    <script type="text/javascript" async src="/js/react/app.js"></script>
    <script type="text/javascript" async src="/js/script.js"></script>

